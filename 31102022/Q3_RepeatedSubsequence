import java.util.ArrayList;
import java.util.Collections;
import java.util.HashMap;
import java.util.Map;

public class Rough {

    static int count = 0;
    public static void main(String[] args) {
            String str = "XYBYAXBY";
        ArrayList<String> output = new ArrayList<>();
        HashMap<String,Integer> map = new HashMap<>();

        output = findSubsequence("",str);
        int count = 0;
        for(String setelement : output)
        {
            for(String tempelement : output)
            {
                if(tempelement.compareTo(setelement + setelement) == 0 && (tempelement.compareTo(setelement) != 0) && setelement.length() >= 2 )
                {
                    count++;
                    map.put(setelement,map.getOrDefault(setelement,0)+1);
                }
            }
        }
        System.out.println("Total count:" + map.size());

    }
    public static ArrayList<String> findSubsequence(String process, String unprocess)
    {
        if(unprocess.isEmpty())
        {
            ArrayList<String> temp = new ArrayList<>();
            temp.add(process);
            return temp;
        }
        char ch  = unprocess.charAt(0);
        ArrayList<String> left = findSubsequence(process+ch,unprocess.substring(1));
        ArrayList<String> right = findSubsequence(process,unprocess.substring(1));

        left.addAll(right);
        return left;
    }
}
