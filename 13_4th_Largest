import java.util.*;
import java.text.*;
class 13_4th_Largest {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter array of elements");
       int arr[] = new int[5];
       
       for(int i =0;i<5;i++)
       {
           arr[i] = sc.nextInt();
       }
        Arrays.sort(arr);
        
        System.out.println("4th largest element is : " + arr[arr.length-4]);
        
        
    }
}
